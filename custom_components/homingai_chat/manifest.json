{
  "domain": "homingai_chat",
  "name": "HomingAI Chat",
  "version": "1.0.0",
  "documentation": "https://www.homingai.com",
  "dependencies": ["frontend", "http", "websocket_api"],
  "codeowners": ["@homingai"],
  "config_flow": true,
  "iot_class": "cloud_polling",
  "requirements": [],
  "homeassistant": "2023.8.0",
  "permissions": ["microphone"],
  "webview_external_urls": [
    "https://homingai.com",
    "https://*.homingai.com",
    "https://api.homingai.com",
    "http://localhost:*",
    "http://127.0.0.1:*"
  ],
  "webview_permissions": {
    "microphone": [
      "https://homingai.com",
      "https://*.homingai.com",
      "https://api.homingai.com",
      "http://localhost:*",
      "http://127.0.0.1:*"
    ]
  }
}